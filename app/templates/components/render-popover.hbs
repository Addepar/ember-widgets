{{#each popover.popovers as |p|}}
  {{#ember-wormhole destinationElement=p.destinationElement}}
    {{#if p.componentName}}
      {{component p.componentName _propertyOptions=p.propertyOptions}}
    {{else}}
      {{view 'popover-container' childViews=p.childViews}}
    {{/if}}
  {{/ember-wormhole}}
{{/each}}
