{
  "name": "ember-widgets",
  "version": "0.0.1",
  "main": [
    "./dist/ember-widgets.js",
    "./dist/ember-widgets.css"
  ],
  "ignore": [
    ".gitignore",
    ".jshintrc",
    "app",
    "build",
    "dependencies",
    "gh_pages",
    "node_modules",
    "src",
    "test",
    "CHANGELOG",
    "example.js",
    "Gruntfile.coffee",
    "package.json",
    "VERSION"
  ],
  "dependencies": {
    "font-awesome": "4.0.1",
    "lodash": "~1.1.1",
    "qunit": "~1.12.0"
  },
  "exportsOverride": {
    "lodash": {
      ".": "lodash.js"
    },
    "font-awesome": {
      "css": "css/*",
      "less": "less/*",
      "scss": "scss/*",
      "fonts": "fonts/*"
    }
  }
}
